
@{
    ViewBag.Title = "Ref";
    Layout = "~/Views/Shared/View.cshtml";
}

<h2>Ref</h2>

<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Poition</th>
        <th>Course</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.name</td>
            <td>@item.position</td>
            <td>@item.Course</td>
        </tr>
    }
</table>